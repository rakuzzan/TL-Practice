{
  "name": "lab3-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "run-s lint:*",
    "lint:cspell": "cspell --no-progress --no-summary .",
    "lint:eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --check --log-level warn .",
    "lint:stylelint": "stylelint --max-warnings=0 src/**/*.css",
    "prettier:fix": "prettier --write .",
    "preview": "vite preview",
    "stylelint:fix": "stylelint --fix src/**/*.css"
  },
  "dependencies": {
    "chart.js": "^4.3.2",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cspell": "^6.31.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "stylelint": "^15.10.1",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
